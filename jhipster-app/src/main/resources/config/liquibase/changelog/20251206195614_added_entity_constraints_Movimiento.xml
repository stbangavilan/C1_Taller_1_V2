<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Movimiento.
    -->
    <changeSet id="20251206195614-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="producto_id"
                                 baseTableName="movimiento"
                                 constraintName="fk_movimiento__producto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"
                                 />

        <addForeignKeyConstraint baseColumnNames="local_origen_id"
                                 baseTableName="movimiento"
                                 constraintName="fk_movimiento__local_origen_id"
                                 referencedColumnNames="id"
                                 referencedTableName="local"
                                 />

        <addForeignKeyConstraint baseColumnNames="local_destino_id"
                                 baseTableName="movimiento"
                                 constraintName="fk_movimiento__local_destino_id"
                                 referencedColumnNames="id"
                                 referencedTableName="local"
                                 />
    </changeSet>
</databaseChangeLog>
